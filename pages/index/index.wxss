/**index.wxss**/
.index_swiper swiper {
  width: 750rpx;
  height: 340rpx;
}
.index_swiper swiper image {
  width: 100%;
}
.naviagtor_index {
  display: flex;
}
.naviagtor_index navigator {
  flex: 1;
  padding: 20rpx;
}
.naviagtor_index navigator image {
  width: 100%;
}
.floor_index .floor_group .floor_title {
  padding: 10rpx 0;
}
.floor_index .floor_group .floor_title image {
  width: 100%;
}
.floor_index .floor_group .floor_main {
  overflow: hidden;
}
.floor_index .floor_group .floor_main navigator {
  float: left;
  width: 33.33%;
}
.floor_index .floor_group .floor_main navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.floor_index .floor_group .floor_main navigator:nth-child(2),
.floor_index .floor_group .floor_main navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.floor_index .floor_group .floor_main navigator image {
  width: 100%;
  height: 100%;
}
